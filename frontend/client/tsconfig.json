{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "jsx": "react-jsx",
      "allowJs": true,
      "checkJs": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noEmit": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  